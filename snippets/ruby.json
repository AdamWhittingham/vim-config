{
  "Private method": {
    "prefix": "pdef",
    "body": [
      "private def $1",
      "  $2",
      "end"
    ]
  },
  "Lazy Load": {
    "prefix": "ll",
    "body": [ "lazy_load(:$1) { $2 }" ]
  },
  "Lazy Entity": {
    "prefix": "le",
    "body": [ "lazy_entity(:$1) { $2 }" ]
  },
  "Lazy Entities": {
    "prefix": "les",
    "body": [ "lazy_entities(:$1) { $2 }" ]
  }
}
